{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build/bundled",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "sitemap.xml",
        "destination": "/sitemap.html"
      },
      {
        "source": "robots.txt",
        "destination": "/robots.txt"
      }
    ]
  }
}
